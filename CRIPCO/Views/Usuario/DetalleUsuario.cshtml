@model CRIPCO.Models.Usuario.DetalleUsuarioViewModel

@{
    ViewBag.Title = "DetalleUsuario";
}


<h2>
 <span class="hidden-xs">Perfil de  @User.Identity.Name</span>
 </h2>
        <img src="~/assets/img/user-6.jpg" alt="" />

        <div>
            @* <h4>DetalleUsuarioViewModel</h4>*@
            <hr />
            <dl class="dl-horizontal">

                <dt>
                    <th>Nombre:</th>
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Nombre)
                </dd>

                <dt>
                    <th>Apellido: </th>
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Apellido)
                </dd>

                <dt>
                    <th>Numero de Identidad:</th>
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Identidad)
                </dd>

                <dt>
                    <th>Fecha de Nacimiento:</th>
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.FechaNac)
                </dd>

                <dt>
                    <th>Numero de Telefono:</th>
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Telefono)
                </dd>

            </dl>
        </div>

