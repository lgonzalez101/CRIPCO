    <script type="text/javascript">
        $(document).ready(function () {

            if ($("#SelectedTipo :selected").val() == "M") {
                $("#Descripcion").hide();
                $("#CargarImagenes").hide();
                $("#ImagenesCargadas").hide();
            }
            else {

                $("#Descripcion").show();
                $("#CargarImagenes").show();
